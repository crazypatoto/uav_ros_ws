            <param name="collision_avoidance_enabled" value="true" />
            <param name="uca_server_ns" value="server2" />
        </node>

        <!-- UAV Mission Handler -->
        <!-- <node pkg="uav_mission_handler" type="uav_mission_handler_node" name="uav_mission_handler" output="screen">
